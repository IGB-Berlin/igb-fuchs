{
  "extends": "..",
  "include": [ "**/*.ts", "**/*.tsx", "types/igb-fuchs.schema.json" ],
  "compilerOptions": {
    "composite": true,
    "target": "ES2022",
    "lib": ["ES2022","DOM"],
    "module": "ES2022",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": [
      "@types/webpack-env",
      "@types/w3c-web-serial",
    ],
    "jsx": "react",
    "jsxFactory": "jsx",
    "jsxFragmentFactory": "jsxFragment",
  }
}