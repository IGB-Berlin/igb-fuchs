{
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "makefile.configureOnOpen": false,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "indentRainbow.ignoreErrorLanguages": [
    "markdown"
  ],
  "cSpell.language": "de,de-DE,en,en-US",
  "cSpell.words": [ "Hauke", "Dämpfling", "Daempfling" ],
  "cSpell.ignoreWords": [
    "Ecohydrology",
    "Gewässerökologie",
    "IDBP",
    "Müggelseedamm",
    "fuchs",
    "hideable",
    "leibniz",
    "papaparse",
    "tseslint",
    "unitless",
    "unparse"
  ],
  "jest.runMode": {
    "type": "on-demand"
  },
  "githubIssues.queries": [
    {
      "label": "Open Issues",
      "query": "repo:${owner}/${repository} is:issue state:open sort:updated-desc",
    },
    {
      "label": "Closed Issues",
      "query": "repo:${owner}/${repository} is:issue state:closed sort:updated-desc",
    },
    {
      "label": "Milestone v0.4.0-beta",
      "query": "repo:${owner}/${repository} milestone:v0.4.0-beta sort:updated-desc",
    },
  ]
}
/* vim: set filetype=javascript ts=2 sw=2 expandtab : */
